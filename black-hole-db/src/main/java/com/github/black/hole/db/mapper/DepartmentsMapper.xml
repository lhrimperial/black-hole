<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.github.black.hole.db.mapper.DepartmentsMapper">

    <sql id="all_column">
        dept_no,dept_name
    </sql>

    <select id="findAll" resultType="com.github.black.hole.db.domain.Departments">
        select <include refid="all_column"/>
        from departments
    </select>
</mapper>
